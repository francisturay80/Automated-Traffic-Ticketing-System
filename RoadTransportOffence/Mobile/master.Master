    <!-- Color Switcher -->
    <script type="text/javascript">
        $(function () {

            $("head").append('<link id="skin-css" rel="stylesheet">');

            $('.skin-colors li').on('click', function () {
                $('.skins li').removeClass('active');
                $(this).addClass('active');
                $('#skin-css').attr('href', 'css/colors/' + $(this).data('skin') + '.css');
            });

        });
    </script>
